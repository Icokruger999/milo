{
  "InstanceIds": ["i-06bc5b2218c041802"],
  "DocumentName": "AWS-RunShellScript",
  "Parameters": {
    "commands": [
      "cd /tmp",
      "curl -L -o Milo.API.dll.ses https://raw.githubusercontent.com/Icokruger999/milo/main/Milo.API.dll.ses",
      "curl -L -o milo-board.html https://raw.githubusercontent.com/Icokruger999/milo/main/frontend/milo-board.html",
      "ls -lh Milo.API.dll.ses milo-board.html",
      "sudo systemctl stop milo-backend",
      "sudo cp /home/ec2-user/milo-backend-publish/Milo.API.dll /home/ec2-user/milo-backend-publish/Milo.API.dll.backup-$(date +%Y%m%d-%H%M%S)",
      "sudo cp Milo.API.dll.ses /home/ec2-user/milo-backend-publish/Milo.API.dll",
      "sudo cp milo-board.html /var/www/html/milo-board.html",
      "sudo systemctl start milo-backend",
      "sleep 3",
      "sudo systemctl status milo-backend --no-pager",
      "curl -s http://localhost:8080/api/health"
    ]
  }
}

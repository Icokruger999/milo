{
  "commands": [
    "cd /home/ec2-user",
    "sudo systemctl stop milo-backend.service",
    "git clone https://github.com/Icokruger999/milo.git temp-repo",
    "cd temp-repo/backend/Milo.API",
    "dotnet publish --configuration Release --output ./publish",
    "cd /home/ec2-user",
    "mkdir -p milo-backend-publish",
    "cp -r temp-repo/backend/Milo.API/publish/* milo-backend-publish/",
    "rm -rf temp-repo",
    "ls -la milo-backend-publish/ | head -n 15",
    "sudo systemctl start milo-backend.service",
    "sleep 5",
    "sudo systemctl status milo-backend.service --no-pager | head -n 20"
  ]
}

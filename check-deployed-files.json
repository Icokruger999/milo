{
  "InstanceIds": ["i-06bc5b2218c041802"],
  "DocumentName": "AWS-RunShellScript",
  "Comment": "Check deployed files",
  "Parameters": {
    "commands": [
      "ls -la /var/www/milo-api/ 2>&1 | head -20",
      "echo ---",
      "ls -la /var/www/milo-api/Controllers/ 2>&1",
      "echo ---",
      "find /var/www/milo-api/ -name '*Flakes*' 2>&1",
      "echo ---",
      "cat /var/www/milo-api/Milo.API.dll 2>&1 | grep -a 'Flakes' | head -5 || echo 'No Flakes found in DLL'"
    ]
  }
}


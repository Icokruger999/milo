{
  "commands": [
    "cd /home/ec2-user",
    "sudo systemctl stop milo-backend.service",
    "git clone https://github.com/Icokruger999/milo.git temp-repo",
    "mkdir -p milo-backend-publish",
    "cp -r temp-repo/backend/Milo.API/publish/* milo-backend-publish/",
    "rm -rf temp-repo",
    "sudo systemctl start milo-backend.service",
    "sleep 5",
    "sudo systemctl status milo-backend.service --no-pager"
  ]
}
